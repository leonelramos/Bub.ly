{
   "manifest_version": 2,
   "name": "bubbly",
   "description": "Bub.ly will analyze the current tab image color distribution.",
   "version": "0.0.2",
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": ["content.js"]
      }
   ],
   "permission": ["tabs"],
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": "bubly_icon.png",
      "default_popup": "popup.html"
   }
}
