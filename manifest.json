{
   "manifest_version": 2,
   "name": "bubbly",
   "description": "Draws colored bubbles proportional (in size) to the number of pixels of the same color.",
   "version": "0.0.2",
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": ["content.js"]
      }
   ],
   "background": {
      "scripts": ["background.js"]
   },
   "browser_action": {
      "default_icon": "bubly_icon.png"
   }
}
